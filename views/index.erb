
<h1>GIT LAUNCHED &#x1F680;</h1>
<% users.each do |user, starred_repos| %>
  <ul>
    <li><%= user %> really likes these repos</li>
      <ul>  <% unless starred_repos.empty? %>
              <% starred_repos.each do |repo| %>
                <li><a href="<%= repo["html_url"]%>"><%= repo["name"] %></a></li>
              <% end %>
            <% end %>
      </ul>
  </ul>
<% end %>
