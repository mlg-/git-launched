<h1>GIT LAUNCHED &#x1F680;</h1>
<ul>
  <% popular_repos.each do |repo| %>
  <li><a href="<%= repo["url"] %>"><%= repo["name"] %></a>
    is described as "<%= repo["description"] %>"
    <% if repo["users"].count > 1 %>
    <ul>Liked by:
      <% repo["users"].each do |user| %>
        <li><img class="icon" src="<%= launchers["#{user}"] %>"/><a href="http://github.com/<%= user %>"><%= user %></li>
      <% end %>
    </ul>
    <% else %>
      <%= repo["users"][0] %> likes this.
    <% end %>
  </li>
  <% end %>
</ul>
